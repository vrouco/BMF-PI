isbn = {00664308},
issn = {0066-4308},
journal = {Annual Review of Psychology},
number = {1},
pages = {417--440},
pmid = {9102251403},
title = {{Personality Structure: Emergence of the Five-Factor Model}},
volume = {41},
year = {1990}
}
